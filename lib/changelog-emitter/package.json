{
  "name": "typespec-versioning-changelog",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "catalog:",
    "@typespec/versioning": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "catalog:",
    "@typespec/prettier-plugin-typespec": "^1.5.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.15.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm build",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier .  --write",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --report-unused-disable-directives --max-warnings=0",
    "check:types": "tsc --noEmit",
    "checks": "pnpm run check:lint && pnpm run check:format && pnpm run check:types",
    "test:coverage": "vitest run --coverage --coverage.reporter=text",
    "audit": "pnpm audit"
  }
}
