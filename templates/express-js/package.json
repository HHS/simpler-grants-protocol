{
  "name": "common-grants-express-js-template",
  "version": "0.1.0",
  "private": true,
  "description": "CommonGrants Express.js template",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf dist generated",
    "typespec": "tsp compile src/typespec/main.tsp",
    "dev": "npm run typespec && ts-node-dev --respawn src/api/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "prepare": "npm run typespec && npm run build",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write . && tsp format src/typespec",
    "check:lint": "eslint . --ext .ts",
    "check:format": "prettier --check . && tsp format src/typespec --check",
    "checks": "npm run check:lint && npm run check:format"
  },
  "keywords": [
    "typespec",
    "api",
    "grants",
    "california"
  ],
  "author": "Agile Six",
  "license": "MIT",
  "dependencies": {
    "@common-grants/core": "^0.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
