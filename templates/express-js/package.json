{
  "name": "common-grants-express-js-template",
  "version": "0.1.0",
  "private": true,
  "description": "CommonGrants Express.js template",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist generated",
    "typespec": "tsp compile src/typespec/main.tsp",
    "dev": "pnpm run typespec && tsx watch src/api/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "prepare": "pnpm run typespec && pnpm run build",
    "lint": "eslint --fix",
    "format": "prettier --write . && tsp format src/typespec",
    "check:lint": "eslint",
    "check:format": "prettier --check . && tsp format src/typespec --check",
    "checks": "pnpm run check:lint && pnpm run check:format"
  },
  "keywords": [
    "typespec",
    "api",
    "grants",
    "california"
  ],
  "author": "Agile Six",
  "license": "MIT",
  "dependencies": {
    "@common-grants/core": "^0.2.3",
    "cors": "^2.8.6",
    "express": "^4.22.1",
    "js-yaml": "^4.1.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.30",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typespec/compiler": "~1.5.0",
    "@typespec/http": "~1.5.0",
    "@typespec/json-schema": "~1.5.0",
    "@typespec/openapi": "~1.5.0",
    "@typespec/openapi3": "~1.5.0",
    "@typespec/rest": "~0.75.0",
    "@typespec/versioning": "~0.75.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^3.2.4"
  }
}
