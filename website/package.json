{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run typespec && astro dev",
    "start": "npm run typespec && astro dev",
    "build": "npm run typespec && astro build",
    "preview": "astro build && astro preview",
    "preview:wrangler": "npm run build && wrangler dev",
    "astro": "astro",
    "typespec": "npm run typespec:clear-output && npm run typespec:openapi && npm run typespec:json-schema",
    "typespec:openapi": "tsp compile src/specs/main.tsp && cp tsp-output/@typespec/openapi3/** public/openapi",
    "typespec:json-schema": "tsp compile src/specs/main.tsp && cp tsp-output/@typespec/json-schema/** public/schemas/yaml",
    "typespec:clear-output": "rm -rf tsp-output && rm -rf public/schemas/yaml/* && rm -rf public/openapi/*",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "check:lint": "eslint",
    "check:format": "prettier --check .",
    "check:astro": "astro check",
    "check:spelling": "cspell .",
    "checks": "npm run check:lint && npm run check:format && npm run check:astro && npm run check:spelling",
    "audit:moderate": "npm audit --audit-level=moderate",
    "audit:high": "npm audit --audit-level=high",
    "audit:critical": "npm audit --audit-level=critical"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.1",
    "@astrojs/starlight": "^0.34.0",
    "@common-grants/core": "^0.2.1",
    "@jsonforms/core": "^3.5.1",
    "@jsonforms/react": "^3.5.1",
    "@jsonforms/vanilla-renderers": "^3.5.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "ajv": "^8.17.1",
    "astro": "^5.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.32.5",
    "swagger-ui-react": "^5.20.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/ajv": "^0.0.5",
    "@types/swagger-ui-react": "^5.18.0",
    "cspell": "^8.17.1",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "starlight-links-validator": "^0.14.1",
    "typescript-eslint": "^8.18.2",
    "wrangler": "^4.27.0"
  }
}
