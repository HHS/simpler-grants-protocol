{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run typespec && npm run generate && astro dev",
    "start": "npm run typespec && npm run generate && astro dev",
    "build": "npm run typespec && npm run generate && astro build",
    "preview": "astro build && astro preview",
    "preview:wrangler": "npm run build && wrangler dev",
    "astro": "astro",
    "typespec": "npm run typespec:clear-output && npm run typespec:compile && npm run typespec:openapi && npm run typespec:json-schema",
    "typespec:compile": "tsp compile src/specs/main.tsp",
    "typespec:openapi": "cp tsp-output/@typespec/openapi3/** public/openapi",
    "typespec:json-schema": "cp tsp-output/@typespec/json-schema/** public/schemas/yaml",
    "typespec:clear-output": "rm -rf tsp-output && rm -rf public/schemas/yaml/* && rm -rf public/openapi/*",
    "generate": "npm run generate:schema-docs && npm run generate:schema-metadata && npm run generate:type-formatting && npm run generate:versioned-schemas",
    "generate:schema-docs": "tsx src/scripts/generate-schema-docs.ts",
    "generate:schema-metadata": "tsx src/scripts/generate-schema-metadata.ts",
    "generate:type-formatting": "tsx src/scripts/generate-type-formatting.ts",
    "generate:versioned-schemas": "tsx src/scripts/generate-versioned-schemas.ts",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "check:lint": "eslint",
    "check:format": "prettier --check .",
    "check:astro": "astro check",
    "check:spelling": "cspell .",
    "checks": "npm run check:lint && npm run check:format && npm run check:astro && npm run check:spelling",
    "audit:moderate": "npm audit --audit-level=moderate",
    "audit:high": "npm audit --audit-level=high",
    "audit:critical": "npm audit --audit-level=critical",
    "validate:schemas": "tsx src/scripts/validate-schemas.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/react": "^4.4.2",
    "@astrojs/starlight": "^0.36.2",
    "@common-grants/core": "^0.3.0-alpha.1",
    "@jsonforms/core": "^3.5.1",
    "@jsonforms/react": "^3.5.1",
    "@jsonforms/vanilla-renderers": "^3.5.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "ajv": "^8.17.1",
    "astro": "^5.16.0",
    "js-yaml": "^4.1.1",
    "openapi-sampler": "^1.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.32.5",
    "swagger-ui-react": "^5.20.2",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2",
    "typespec-versioning-changelog": "file:../lib/changelog-emitter"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/ajv": "^0.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/swagger-ui-react": "^5.18.0",
    "cspell": "^8.17.1",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "starlight-links-validator": "^0.14.1",
    "typescript-eslint": "^8.18.2",
    "vitest": "^4.0.10",
    "wrangler": "^4.27.0"
  },
  "overrides": {
    "js-yaml": "^4.1.1"
  }
}
