{
  "organizations": {
    "primary": {
      "id": { "field": "OrganizationName" },
      "name": { "field": "OrganizationName" }
    }
  },
  "contacts": {
    "primary": {
      "name": {
        "prefix": { "field": "contacts.Prefix" },
        "firstName": { "field": "contacts.FirstName" },
        "middleName": { "field": "contacts.MiddleName" },
        "lastName": { "field": "contacts.LastName" },
        "suffix": { "field": "contacts.Suffix" }
      },
      "title": { "field": "contacts.Title" },
      "addresses": {
        "primary": {
          "street1": { "field": "contacts.Street1" },
          "street2": { "field": "contacts.Street2" },
          "city": { "field": "contacts.City" },
          "stateOrProvince": { "field": "contacts.State" },
          "country": { "field": "contacts.Country" },
          "postalCode": { "field": "contacts.ZipPostalCode" }
        }
      },
      "phones": {
        "primary": {
          "countryCode": "+1",
          "number": { "field": "contacts.PhoneNumber" }
        },
        "otherPhones": {
          "fax": {
            "countryCode": "+1",
            "number": { "field": "contacts.Fax" }
          }
        }
      },
      "emails": {
        "primary": { "field": "contacts.Email" }
      }
    }
  },
  "customFields": {
    "organizationAffiliation": {
      "name": "organizationAffiliation",
      "fieldType": "string",
      "value": { "field": "contacts.OrganizationAffiliation", "default": "" },
      "description": "Organization affiliation"
    },
    "projectRole": {
      "name": "projectRole",
      "fieldType": "string",
      "value": { "field": "contacts.ProjectRole" },
      "description": "Role in the project"
    }
  }
}
